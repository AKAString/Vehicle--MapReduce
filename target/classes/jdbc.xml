<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="configuration.xsl"?>
<configuration>
<!--使用VMnet8 和Windows进行通信--> 
	<property>
		<name>url</name>
		<value>jdbc:mysql://localhost:3306/guazi?useUnicode=true&amp;characterEncoding=UTF8&amp;allowMultiQueries=true</value>
	</property>
	<property>
		<name>username</name>
		<value>root</value>
	</property>
	<property>
		<name>password</name>
		<value>root</value>
	</property>
	<property>
		<name>driver</name>
		<value>com.mysql.jdbc.Driver</value>
	</property>
</configuration>
